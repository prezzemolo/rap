{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "declaration": true,
    "outDir": "./built",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "pretty": true
  },
  "include": [
    "./src/**/*.ts"
  ]
}
